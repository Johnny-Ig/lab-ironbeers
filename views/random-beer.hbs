<div class="randomBeer">

    {{#each this.random}}
 <img src="{{this.image_url}}" width="250px" alt="cerves">
  <h3>Name: {{this.name}}</h3>
  <p>Description: {{this.description}}</p>
  <p>Tagline: {{this.tagline}}</p>
  <h4>Food pairing:</h4>
  {{#each this.food_pairing}}
   
  <li>{{this}}</li>
  {{/each}}
  <p>Brewers tips: {{this.brewers_tips}}</p>

   {{/each}}



</div>